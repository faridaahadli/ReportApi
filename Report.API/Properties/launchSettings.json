      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Api-Dev": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
