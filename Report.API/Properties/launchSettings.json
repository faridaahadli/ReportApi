        "JWT_ORIGIN": "",
        "OMS_DB_HOST": "",
        "OMS_DB_NAME": "",
        "OMS_DB_PASSWORD": "",
        "OMS_DB_PORT": "",
        "OMS_DB_USERNAME": "",
        "PAYMENT_DB_HOST": "",
        "PAYMENT_DB_NAME": "",
        "PAYMENT_DB_PASSWORD": "",
        "PAYMENT_DB_PORT": "",
        "PAYMENT_DB_USERNAME": "",
        "REPORT_DB_HOST": "",
        "REPORT_DB_NAME": "",
        "REPORT_DB_PASSWORD": "",
        "REPORT_DB_PORT": "",
        "REPORT_DB_USERNAME": ""
      },
      "applicationUrl": "https://localhost:7096;http://localhost:5096"
    },
    "Api-Stage": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
